//Initialization Function
function FindProxyForURL(url, host) {
    //These are my personal excludes just in case I accidently go to a login site with this proxy enabled
    var proxy = "PROXY xenos.nsupdate.info:8899";
    if (isInNet(myIpAddress(), "192.168.2.0", "255.255.255.0"))
        proxy = "PROXY 192.168.2.150:8899";
    else if (shExpMatch(host, "192.168.2.*"))
        return proxy;
    if (ENABLE_INCLUDES && include(host.toLowerCase(), DOMAIN_INCLUDES)) {
        return proxy;
    };
    return "DIRECT";
}
