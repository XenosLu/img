//Custom defined functions for cleaner processing
function include(source, rules) {
    try {
        for (var i = 0; i < rules.length; i++)
            if (shExpMatch(source, rules[i]))
                return true;
        return false;
    } catch (e) {
        if (ENABLE_LOGGING)
            alert("An exception has been encountered!\n" + source + "\n" + rules);
        return false;
    }
}

